[loggers]
keys=root

[handlers]
keys=logFileHandler,consoleHandler

[formatters]
keys=logFileFormatter,consoleFormatter

[logger_root]
level=DEBUG
handlers=consoleHandler
#handlers=logFileHandler
#handlers=logFileHandler,consoleHandler

[handler_logFileHandler]
class=FileHandler
level=DEBUG
formatter=logFileFormatter
args=("/proc/1/fd/1", "w", "utf-8")
#args=("./log/app.log", "w", "utf-8")

[handler_consoleHandler]
class=StreamHandler
level=DEBUG
formatter=consoleFormatter
args=(sys.stdout,)

[formatter_logFileFormatter]
format=%(asctime)s|%(levelname)-8s|%(name)s|%(funcName)s|%(message)s

[formatter_consoleFormatter]
format=%(asctime)s [%(levelname)-8s] %(message)s::%(filename)s::%(lineno)d::%(funcName)s()
#format=[%(levelname)-8s]%(funcName)s - %(message)s